<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="postBody">Post</label>
    <textarea name="postBody" rows="8" cols="80" class="form-control"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
